{
  "name": "loopback-react-redux-antd-starter",
  "version": "0.0.1",
  "main": "server/server.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "postinstall": "npm run build-fe",
    "start": "node .",
    "debug": "npm-run-all -p watch-css debug-fe debug-be",
    "debug-be": "nodemon --inspect .",
    "test": "npm run test-fe",
    "posttest": "npm run lint && nsp check",
    "lint": "eslint .",
    "debug-fe": "react-scripts start",
    "build-fe": "npm run build-css && dotenv -e .env.react react-scripts build",
    "build-css": "node-sass-chokidar src/scss -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o src/css --watch --recursive",
    "test-fe": "cross-env CI=true react-scripts test --env=jsdom"
  },
  "dependencies": {
    "antd": "^3.0.0",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "debug": "^3.1.0",
    "dotenv-cli": "^1.4.0",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "keymirror": "^0.1.1",
    "libphonenumber-js": "^0.4.47",
    "lodash": "^4.17.4",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-connector-mongodb": "^3.3.1",
    "mailgun-js": "^0.14.0",
    "moment": "^2.19.3",
    "mws-api": "^2.1.0",
    "node-sass-chokidar": "^0.0.3",
    "react": "^16.1.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.1.1",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.2",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "xlsx": "^0.11.16"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-loopback": "^8.0.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "react-redux-loopback-template"
}
